    "joi": "^17.6.0",
    "nestjs-cls": "^5.0.1",
    "openai": "^4.80.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
